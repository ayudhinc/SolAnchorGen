{
  "name": "sol-anchor-gen",
  "version": "0.1.0",
  "description": "CLI tool for generating Anchor program scaffolding for Solana development with production-ready templates",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sol-anchor-gen": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
