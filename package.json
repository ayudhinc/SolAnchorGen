    "web3",
    "nft",
    "staking",
    "defi",
    "dao",
    "governance",
    "smart-contracts",
    "solana-program",
    "template",
    "boilerplate"
  ],
  "author": "Dushyant Kashyap <dushyant@ayudh.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayudhinc/SolAnchorGen.git"
  },
  "bugs": {
    "url": "https://github.com/ayudhinc/SolAnchorGen/issues"
  },
  "homepage": "https://github.com/ayudhinc/SolAnchorGen#readme",
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-table3": "0.6.5",
    "commander": "^14.0.2",
    "inquirer": "^12.11.0",
    "ora": "9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
