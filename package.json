{
  "name": "sol-anchor-gen",
  "version": "0.1.0",
  "description": "CLI tool for generating Anchor program scaffolding for Solana development",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "sol-anchor-gen": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "link": "pnpm link --global",
    "unlink": "pnpm unlink --global"
  },
  "keywords": [
    "solana",
    "anchor",
    "cli",
    "scaffold",
    "generator",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.6.2",
    "cli-table3": "0.6.5",
    "commander": "^14.0.2",
    "inquirer": "^12.11.0",
    "ora": "9.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
